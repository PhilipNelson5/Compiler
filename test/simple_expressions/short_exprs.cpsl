  b := 5;
  write("a+b=",a+b,'\n');
  write("a-b=",a-b,'\n');
  write("a*b=",a*b,'\n');
  write("a/b=",a/b,'\n');
  write("a%b=",a%b,'\n');
  write("a=b=",a=b,'\n');
  write("a<b=",a<b,'\n');
  write("a>b=",a>b,'\n');
  write("a<>b=",a<>b,'\n');
  write("a<=b=",a<=b,'\n');
  write("a>=b=",a>=b,'\n');
END.
